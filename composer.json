{
    "name":        "appliora/timezone-bundle",
    "type":        "symfony-bundle",
    "description": "Translation of the time difference for a given timezone",
    "keywords":    ["time", "date", "appliora", "bundle", "app"],
    "homepage":    "http://github.com/dturak/TimezoneBundle",
    "license":     "MIT",

    "authors": [
        {
            "name": "Daniel Turski",
            "homepage": "http://github.com/dturak"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/yaml": "^6.2",
        "symfony/translation": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Appliora\\Bundle\\TimezoneBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appliora\\Bundle\\TimezoneBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "require-dev": {
        "symfony/framework-bundle": "^6.2",
        "symfony/twig-bundle": "^6.2",
        "symfony/phpunit-bridge": "^6.2",
        "phpunit/phpunit": "^10.0"
    }
}
